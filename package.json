{
  "name": "Jenio-CodeTemp",
  "version": "0.0.1",
  "private": true,
  "scripts": {

    "test": "jest",
    "build": "pnpm build:api && pnpm build:web",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "lint:all": "node node_modules/eslint/bin/eslint.js 'src/**/*.{js,jsx,ts,tsx}'",
    "lint": "node node_modules/eslint/bin/eslint.js",

    "run:web": "next start",
    "build:web": "pnpm prepare:web && next build",
    "dev:web": "next dev",
    "prepare:web": "node node_modules/@neur0base/app-sdk-next-router/lib/commonjs/scripts/genAppDir.js",

    "run:storybook": "storybook dev -p 9001",
    "test:storybook": "test-storybook",

    "gen:api:cli": "npx -y @rtk-query/codegen-openapi ./api/openapi-config.json && echo '// @ts-nocheck' > tmpfile && echo 'export { api } from \"./baseApi\";' >> tmpfile && cat ./src/services/client/coreApi.ts >> tmpfile && mv tmpfile ./src/services/client/coreApi.ts",
    "gen:api:docs": "npx -y @redocly/cli build-docs ./src/services/api/schema.json -o ./src/services/api/docs.html",
    "gen:api": "pnpm gen:api:cli && pnpm gen:api:docs",
    "dev:api": "node node_modules/nodemon/bin/nodemon.js --config ./api/nodemon.json",
    "build:api": "pnpm gen:api && node_modules/typescript/bin/tsc",
    "run:api": "node node_modules/nodemon/bin/nodemon.js --config ./api/nodemon.json"

  },
  "dependencies": {
    "@core": "npm:@neur0base/app-sdk-core@^0.0.130",
    "@echopf/sdk": "^2.1.5",
    "@neur0base/app-sdk-core": "^0.0.130",
    "@neur0base/app-sdk-next-router": "^0.0.10",
    "@neur0base/app-sdk-ui": "^0.0.69",
    "@reduxjs/toolkit": "^2.2.6",
    "autoprefixer": "^10.4.19",
    "body-parser": "^1.20.2",
    "child_process": "^1.0.2",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "express-openapi": "^12.1.3",
    "moment": "2.30.1",
    "moment-timezone": "^0.5.45",
    "next": "^14.2.5",
    "postcss": "^8.4.39",
    "python-shell": "^5.0.0",
    "react": "^18.3.1",
    "react-native": "^0.73.9",
    "react-native-web": "^0.19.12",
    "tailwind-rn": "3.0.1",
    "tailwindcss": "^3.4.6",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.24.8",
    "@babel/preset-react": "^7.24.7",
    "@babel/preset-typescript": "^7.24.7",
    "@storybook/addon-essentials": "^8.2.4",
    "@storybook/addon-links": "^8.2.4",
    "@storybook/builder-vite": "^8.2.4",
    "@storybook/react": "^8.2.4",
    "@storybook/react-vite": "^8.2.4",
    "@storybook/test": "^8.2.4",
    "@storybook/test-runner": "^0.17.0",
    "@types/react": "^18.3.3",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.3.1",
    "babel-loader": "^9.1.3",
    "babel-plugin-react-native-web": "^0.19.12",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "file-loader": "^6.2.0",
    "jest": "^29.7.0",
    "metro-react-native-babel-preset": "^0.77.0",
    "nodemon": "^3.1.4",
    "prettier": "^2.8.8",
    "storybook": "^8.2.4",
    "storycap": "^4.3.1",
    "typescript": "^4.9.5",
    "url-loader": "^4.1.1",
    "vite": "^5.3.4"
  },
  "resolutions": {
    "react": "^18.3.1",
    "react-native": "^0.73.8"
  }
}
