{
  "compilerOptions": {
    "outDir": "./dist",
    "paths": {
      "@/*": [
        "./*"
      ],
      "@neur0base/app-sdk-router": [
        "./node_modules/@neur0base/app-sdk-next-router"
      ],
      "@jenify_ai/app-sdk-router": [
        "./node_modules/@neur0base/app-sdk-next-router"
      ],
      "@jenify_ai/app-sdk-core": [
        "./node_modules/@neur0base/app-sdk-core"
      ],
      "@jenify_ai/app-sdk-ui": [
        "./node_modules/@neur0base/app-sdk-ui"
      ],
      "@router": [
        "./node_modules/@neur0base/app-sdk-next-router"
      ],
      "@core": [
        "./node_modules/@neur0base/app-sdk-core"
      ],
      "@ui": [
        "./node_modules/@neur0base/app-sdk-ui"
      ]
    },
    "types": [
      "vite/client"
    ],
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "module": "esnext",
    "moduleResolution": "node",
    "noEmit": true,
    "resolveJsonModule": true,
    "useDefineForClassFields": true,
    "noImplicitAny": false,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "exclude": [
    "node_modules",
    "babel.config.js",
    "metro.config.js",
    "jest.config.js",
    "vite.config.js",
    "./dist/**/*",
    "./workflows/**/*"
  ],
  "include": [
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx"
  ]
}
