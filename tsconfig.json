{
  "compilerOptions": {
    "outDir": "./dist",
    "paths": {
      "@/*": ["./*"],
      "@jenify_ai/*": ["./node_modules/@neur0base/*"],
    },
    "types": ["vite/client"],
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "jsx": "react-native",
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "module": "esnext",
    "moduleResolution": "node",
    "noEmit": true,
    "resolveJsonModule": true,
    "useDefineForClassFields": true,
    "noImplicitAny": false,
    "skipLibCheck": true,
    "strict": false
  },
  "exclude": [
    "node_modules",
    "babel.config.js",
    "metro.config.js",
    "jest.config.js",
    "vite.config.js",
    "./dist/**/*",
    "./workflows/**/*"
  ]
}
